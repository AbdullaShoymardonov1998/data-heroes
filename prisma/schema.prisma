generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id        Int      @id @default(autoincrement())
  name      String
  status    String
  species   String
  type      String
  gender    String
  origin    Json
  location  Json
  createdAt DateTime @default(now()) @map("created_at")
}
